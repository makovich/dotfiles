<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>doh.https_dns_proxy</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/https_dns_proxy</string>
      <string>-b 8.8.8.8,8.8.4.4</string>
      <string>-r "https://dns.google/resolve?"</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
  </dict>
</plist>
